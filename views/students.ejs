<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('partials/head') %>
</head>

<body>
  <% if(students.length === 0) { %>
  <div class="card"><%= "There are no records" %></div>
  <% } %>
  <% students.forEach(student => { %>
  <div class="card">
    <div class="card-body">
      <h5 class="card-title"><%= student.fname + " "+student.lname %></h5>
      <p class="mb-2"><%= student.department %></p>
      <p class="mb-2"><%= student.sem %></p>
      <p class="mb-2"><%= student.usn %></p>
    </div>
  </div>
  <% }) %>
  <%- include('partials/scripts') %>
</body>

</html>